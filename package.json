{
  "name": "proxy-gol-aws",
  "version": "1.0.0",
  "main": "index.js",
  "author": "michal.bures@travelportgds.cz",
  "license": "none",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "tsc --project tsconfig.json && eslint --ext .ts .",
    "autofix": "prettier **/* --write",
    "serverless": "node_modules/.bin/serverless",
    "deploy": "npm run lint && npm run serverless -- deploy",
    "offline": "serverless offline start --stage local",
    "test": "concurrently --raw -k -s first \"npm run offline\" \"wait-on http://localhost:3000/status && jest ./tests\""
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "endOfLine": "lf",
    "printWidth": 120,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": [
            2,
            {
              "args": "none"
            }
          ]
        }
      }
    ]
  },
  "devDependencies": {
    "@travelport-czech/valid-objects-ts": "^1.1.5",
    "@types/aws-lambda": "^8.10.27",
    "@types/jest": "^24.9.0",
    "@types/node": "^8.10.49",
    "@types/node-fetch": "^2.3.7",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.476.0",
    "babel-loader": "^8.0.6",
    "bufferutil": "^4.0.1",
    "cache-loader": "^4.1.0",
    "concurrently": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "extendable-error": "^0.1.5",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "serverless": "^1.45.1",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.12.0",
    "serverless-webpack": "^5.3.1",
    "source-map-support": "^0.5.12",
    "thread-loader": "^2.1.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.2",
    "utf-8-validate": "^5.0.2",
    "uuid": "^3.3.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  }
}
