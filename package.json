{
  "name": "proxy-gol-aws",
  "version": "1.0.0",
  "main": "index.js",
  "author": "michal.bures@travelportgds.cz",
  "license": "none",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "tsc && tslint --project tsconfig.json --config config/tslint.json",
    "autofix": "tslint --project tsconfig.json --config config/tslint.json --fix && prettier ./src/**/* ./tests/**/* --write",
    "serverless": "node_modules/.bin/serverless",
    "deploy": "npm run lint && npm run serverless -- deploy",
    "offline": "serverless offline start --stage local",
    "test": "concurrently --raw -k -s first \"npm run offline\" \"wait-on http://localhost:3000/status && jest ./tests\""
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "endOfLine": "lf",
    "printWidth": 120,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/jest": "^24.9.0",
    "@types/node": "^8.10.49",
    "@types/node-fetch": "^2.3.7",
    "@types/uuid": "^3.4.4",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.476.0",
    "babel-loader": "^8.0.6",
    "bufferutil": "^4.0.1",
    "cache-loader": "^4.1.0",
    "concurrently": "^4.1.0",
    "extendable-error": "^0.1.5",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "serverless": "^1.45.1",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.12.0",
    "serverless-webpack": "^5.3.1",
    "source-map-support": "^0.5.12",
    "thread-loader": "^2.1.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^6.0.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2",
    "utf-8-validate": "^5.0.2",
    "uuid": "^3.3.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  }
}
